#include <stdio.h>

int main() {
    int a,b;
    
    printf("Enter the start of the range: ");
    scanf("%d", &a);
    printf("Enter the end of the range: ");
    scanf("%d", &b);
    
    for (int i = a; i <= b; i++) {
        if (i % 2 == 0)
            printf("%d is Even\n", i);
        else
            printf("%d is Odd\n", i);
    }
    
    return 0;
}
