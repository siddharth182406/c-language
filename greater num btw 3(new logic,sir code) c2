#include<stdio.h>
int main()
{

int num1, num2, num3,result;
printf("enter 3 numbers");
printf("\n num1=");
scanf("%d",&num1);
printf("\n num2=");
scanf("%d",&num2);
printf("\n num3=");
scanf("%d",&num3);
//nested if else
/*if(condition=true)
{
block1;}
else
{block2;
}*/
if ((num1>=num2)&& (num1>=num3))
{
result=num1;
}
if ((num2>=num1)&& (num2>=num3))
{
result=num2;
}
if ((num3>=num1)&& (num3>=num2))
{
result=num3;
}

printf("max(%d,%d,%d)=%d",num1,num2,num3,result);
return 0;
}
