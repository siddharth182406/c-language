// Online C compiler to run C program online
#include <stdio.h>

int main() {
  float num1;
  float num2;
  
  printf("entre cost price:");
  scanf("%f",&num1);
  
  printf("entre selling price:");
  scanf("%f",&num2);
  
  if (num1>num2)
    printf("you are in loss");
  else
    printf("you are in profit");
    
  if (num1 == num2)
    printf("no profit no loss");

    return 0;
}
