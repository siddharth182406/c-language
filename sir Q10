#include <stdio.h>

int main() {
  char c;
    
    printf("entre alphabate:");
    scanf("%c",&c);
    
    if (c >= 'a' && c <= 'z') 
     printf(" %c  alphabate is an lowwer case ",c);
    
    
    else if (c >= 'A' && c <= 'Z') 
      printf(" %c  alphabate is an upper case ",c);
    
    else 
     printf("%c is not an alphabate",c);

    return 0;
}
