// if you use read 'r' it will read the created file in you folder and if you  write 'w' it eill create new folder 
#include <stdio.h>      

int main() {
    
    FILE*ptr;
    ptr = fopen("sidd.txt","r");  // write the file name you have made 
    int num;
    fscanf(ptr,"%d",&num);  // it will print the single value you have written in that file 
    printf("The value of num is %d \n",num)
  
   return 0;
}
