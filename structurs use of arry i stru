#include <stdio.h>
#include <string.h>

struct employee
{
    int code; // this declare the new user define data type
    float salary;
    char name[10];
}; // semicoloen is imp

int main() {
    struct employee facebook[100]; // like this you can use arrays, and you can write till facebook[99].code 
    facebook[0].code = 100;
    facebook[1].code = 101; 
    printf("%d %d",facebook[0].code,facebook[1].code);
    
    return 0;
}
