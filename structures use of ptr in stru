#include <stdio.h>
#include <string.h>

struct employee
{
    int code; // this declare the new user define data type
    float salary;
    char name[10];
}; // semicoloen is imp

int main() {
    struct employee e1;  
    e1.code = 24;
    struct employee *ptr;
    ptr = &e1;
    // Now we can print stuct element using
    printf("%d",(*ptr).code);
    
    return 0;
}
