// Online C compiler to run C program online
#include <stdio.h>

int main() {
  int num;
    
    printf("entre the num:");
    scanf("%d",&num);
    
    if (num % 5 == 0 && num % 11 == 0)
     printf(" %d  is divisible of 5 and 11",num);
     
    else 
     printf("%d is not divisible by 5 and 11",num);

    return 0;
}
