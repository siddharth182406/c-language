#include <stdio.h>
#include <string.h>

typedef struct employee 
{
    int code; // this declare the new user define data type
    float salary;
    char name[10];
} Emp; // semicoloen is imp

int main() {
    Emp e1;  
    e1.code = 24;
    strcpy(e1.name,"siddharth");
    e1.salary = 24.24;
    
    printf("%d %s %f",e1.code,e1.name,e1.salary);
    
    return 0;
}
