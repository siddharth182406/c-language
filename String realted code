
#include <stdio.h>

int main() {

char st[] ="abc"; //the null char will print directily
for(int i=0;i<=3;i++){
    printf("character is %c\n",st[i]);
}

    return 0;
}


/*
if you want to write directly abc you can just write 
#include <stdio.h>

int main() {

char st[] ="abc"; //the null char will print directily

printf("%s",st);


    return 0;
}
*/
